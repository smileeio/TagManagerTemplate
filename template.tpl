// Enter your template code here.
const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');

const apiKey = data.apiKey;
const smileeUrl = 'https://saas.smilee.fi/assets/javascripts/cobrowse.js';

// If the script loaded successfully, log a message and signal success
const onSuccess = () => {
  log('Smilee: Script loaded successfully.');
  const Cobrowse = copyFromWindow('Cobrowse');
  Cobrowse.create({
    apiKey: apiKey,
    serverUrl: 'https://saas.smilee.fi'
  });
  data.gtmOnSuccess();
};

// If the script fails to load, log a message and signal failure
const onFailure = () => {
  log('Smilee: Script load failed.');
  data.gtmOnFailure();
};

injectScript(smileeUrl, onSuccess, onFailure, smileeUrl);


